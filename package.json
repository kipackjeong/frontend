{
  "name": "choseong-bingo-frontend",
  "version": "1.0.0",
  "description": "Frontend for 초성빙고 (Choseong Bingo) - Multilingual multiplayer word bingo game",
  "main": "index.ts",
  "scripts": {
    "start": "expo start --clear",
    "start:dev": "expo start --dev-client --clear",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "ios:device": "expo start --ios --device",
    "ios:simulator": "expo start --ios --simulator",
    "web": "expo start --web",
    "build": "expo build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.{ts,tsx}",
    "lint:fix": "eslint src/**/*.{ts,tsx} --fix",
    "type-check": "tsc --noEmit",
    "clean": "expo r -c",
    "reset-cache": "expo r -c && npm start -- --reset-cache"
  },
  "dependencies": {
    "@expo/metro-runtime": "~4.0.1",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-navigation/native": "^6.1.18",
    "@react-navigation/stack": "^6.4.1",
    "camelcase-keys": "^9.1.3",
    "clipboard": "^2.0.11",
    "expo": "~52.0.0",
    "expo-asset": "^12.0.9",
    "expo-clipboard": "^7.1.5",
    "expo-linear-gradient": "^14.1.5",
    "expo-status-bar": "~2.0.1",
    "hangul-js": "^0.2.6",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-native": "0.76.9",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.4.0",
    "react-native-svg": "^15.12.0",
    "react-native-vector-icons": "^10.2.0",
    "react-native-web": "~0.19.10",
    "socket.io-client": "^4.7.4",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@testing-library/jest-native": "^5.4.0",
    "@testing-library/react-native": "^12.4.0",
    "@types/jest": "^30.0.0",
    "@types/react": "~18.3.12",
    "@types/react-native": "~0.73.0",
    "@types/react-navigation": "^3.0.8",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.7.0",
    "react-test-renderer": "18.3.1",
    "typescript": "~5.3.3"
  },
  "keywords": [
    "bingo",
    "korean",
    "english",
    "multiplayer",
    "game",
    "react-native",
    "expo",
    "typescript"
  ],
  "author": "Choseong Bingo Team",
  "license": "MIT",
  "private": true
}